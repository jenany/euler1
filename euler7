primes = [2,3]
count = 2
def is_prime(n):
    limit = int(n**.5)+1
    for item in primes[1:]:
        if n%item == 0:
            return False
        if item > limit:
            break
    return True

while len(primes) < 10001:
    if is_prime(primes[-1]+2):
        primes.append(primes[-1]+2)
else:
    answer = primes[-1]

print answer
